{
  "importOrder": [
    "^reflect-metadata",
    "<THIRD_PARTY_MODULES>",
    "^[./]",
    "^@/(.*)$"
  ],
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "decorators-legacy"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "printWidth": 120,
  "quoteProps": "consistent",
  "singleQuote": true,
  "tailwindFunctions": [
    "classNames",
    "cn"
  ]
}